{
    "semi": true,
    "singleQuote": true,
    "tabWidth": 2,
    "useTabs": false,
    "endOfLine": "auto",
    "printWidth": 80,
    "arrowParens": "avoid",
    "proseWrap": "always",
    "htmlWhitespaceSensitivity": "strict",
    "jsxBracketSameLine": false,
    "bracketSpacing": true,
    "insertPragma": false,
    "requirePragma": false,
    "quoteProps": "as-needed",
    "trailingComma": "none",
    "eslintIntegration": true,
    "jsxSingleQuote": false,
    "vueIndentScriptAndStyle": false
}