{
  "name": "react-navigable-list",
  "version": "0.0.1",
  "type": "module",
  "description": "",
  "homepage": "https://github.com/sadiki-o/react-navigable-list#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/sadiki-o/react-navigable-list.git"
  },
  "author": "Oussama Sadiki oussam.sadiki@gmail.com",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/sadiki-o/react-navigable-list/issues"
  },
  "files": [
    "src/lib/**/*.js",
    "dist/*.js",
    "dist/{themes,styled,unstyled,full}.css",
    "src/lib/index.ts"
  ],
  "main": "./dist/react-navigable-list.es.js",
  "module": "./dist/react-navigable-list.umd.js",
  "types": "./dist/index.d.ts",
  "sideEffects": false,
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/react-navigable-list.es.js",
      "require": "./dist/react-navigable-list.umd.js"
    },
    "./dist/style.css": "./dist/style.css"
  },
  "keywords": [
    "react-select",
    "react-list",
    "react-scrollable-list",
    "react-custom-select",
    "react-dropdown",
    "react-navigation",
    "react-options"
  ],
  "scripts": {
    "dev": "vite",
    "clean:lib": "rm -rf dist",
    "build:lib": "vite build",
    "build": "run-s clean:lib build:lib",
    "lint:code": "eslint src/lib/**/** --ext js,jsx,ts,tsx --fix",
    "preview": "vite preview",
    "prepare": "husky || true"
  },
  "dependencies": {
    "clsx": "^2.1.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-virtualized": "^9.22.5"
  },
  "devDependencies": {
    "@commitlint/cli": "^19.2.1",
    "@commitlint/config-conventional": "^19.1.0",
    "@types/node": "^20.12.7",
    "@types/react": "^18.2.78",
    "@types/react-dom": "^18.2.25",
    "@types/react-virtualized": "^9.21.30",
    "@vitejs/plugin-react": "^4.2.1",
    "autoprefixer": "^10.4.19",
    "eslint": "^8.57.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-node": "^4.1.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-jsx-a11y": "^6.8.0",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^5.1.3",
    "eslint-plugin-react": "^7.34.1",
    "eslint-plugin-react-hooks": "^4.6.0",
    "husky": "^9.0.11",
    "postcss": "^8.4.38",
    "prettier": "^3.2.5",
    "tailwindcss": "^3.4.3",
    "typescript": "^5.4.5",
    "vite": "^5.2.8",
    "vite-plugin-css-injected-by-js": "^3.5.1",
    "vite-plugin-dts": "^3.8.2"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{js,jsx}": [
      "eslint --fix",
      "prettier --write",
      "git add"
    ]
  }
}
